{
  "private": "true",
  "name": "hibiki",
  "version": "3.3.0-alpha",
  "description": "An easy, versatile, and all-in-one Discord bot that does anything your server needs.",
  "author": "Espi <espi@riseup.net> (https://espi.me)",
  "contributors": [
    "resolved <resolvedxd@gmail.com> (https://github.com/resolvedxd)"
  ],
  "license": "AGPL-3.0-or-later",
  "homepage": "https://hibiki.app",
  "main": "dist/src/index.js",
  "bugs": {
    "url": "https://github.com/smolespi/hibiki/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smolespi/hibiki.git"
  },
  "engines": {
    "npm": ">=6.9.0",
    "node": ">=14.0.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "cross-env nodemon --ext 'ts' --watch 'src' --delay 5 --ignore 'rethinkdb_data/*' --exec 'cross-env NODE_ENV=development ts-node --files ./src/index.ts'",
    "format": "cross-env prettier --config .prettierrc 'src/**/*.{css,htm,html,js,json,ts,tsx}' --write",
    "lint": "eslint --fix .",
    "start": "tsc && cross-env NODE_ENV=production node dist/src/index.js",
    "setup": "ts-node --files ./src/utils/setup.ts",
    "test": "eslint ."
  },
  "dependencies": {
    "@sentry/node": "^5.29.2",
    "axios": "^0.21.1",
    "dayjs": "^1.10.2",
    "erela.js": "^2.3.1",
    "erela.js-spotify": "^1.2.0",
    "eris": "^0.14.0",
    "rethinkdb-ts": "^2.4.5",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@sentry/types": "^5.29.2",
    "@types/node": "^14.14.20",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "optionalDependencies": {
    "@discordjs/opus": "^0.3.3",
    "bufferutil": "^4.0.3",
    "erlpack": "^0.1.3",
    "eventemitter3": "^4.0.7",
    "zlib-sync": "^0.1.7"
  }
}
